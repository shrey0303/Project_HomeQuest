{
  "version": 2,
  "builds": [
    {
      "src": "dist/api/graphql.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "Access-Control-Allow-Origin": "https://home-quest-mu.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
